{
  "version": 2,
  "name": "zk-pret-frontend",
  "framework": null,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(css|js|html|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@20",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production",
    "OPERATION_MODE": "HTTP",
    "ENABLE_ASYNC_JOBS": "true",
    "ENABLE_WEBSOCKETS": "false",
    "ENABLE_DEMO_MODE": "false",
    "ENABLE_MOCK_RESPONSES": "false"
  }
}