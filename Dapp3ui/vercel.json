{
  "version": 2,
  "name": "zkpretui",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "OPERATION_MODE": "HTTP",
    "ZK_PRET_SERVER_URL": "https://zkpretserver.vercel.app",
    "ZK_PRET_HTTP_SERVER_URL": "https://zkpretserver.vercel.app",
    "HTTP_SERVER_URL": "https://zkpretserver.vercel.app",
    "CORS_ORIGIN": "https://zkpretui.vercel.app",
    "ENABLE_ASYNC_JOBS": "false",
    "ENABLE_WEBSOCKETS": "false",
    "ZK_PRET_SERVER_TIMEOUT": "30000"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}